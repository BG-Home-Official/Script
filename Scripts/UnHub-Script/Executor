	["NumberSequenceKeypoint"] = "#A2FD99", ["ColorSequence"] = "#A2FD99",
	["ColorSequenceKeypoint"] = "#A2FD99", ["Faces"] = "#A2FD99", ["Axes"] = "#A2FD99",
	["Rect"] = "#A2FD99", ["Region3"] = "#A2FD99", ["Region3int16"] = "#A2FD99",
	["TweenInfo"] = "#A2FD99", ["PathWaypoint"] = "#A2FD99", ["DockWidgetPluginGuiInfo"] = "#A2FD99",

	-- Roblox functions/methods (blue)
	["FindFirstChild"] = "#2241FF", ["FindFirstAncestor"] = "#2241FF", ["WaitForChild"] = "#2241FF",
	["GetService"] = "#2241FF", ["IsA"] = "#2241FF", ["Clone"] = "#2241FF",
