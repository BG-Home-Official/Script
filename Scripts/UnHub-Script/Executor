local keywords = {
	-- Core Lua keywords
	["and"] = "#B800B3", ["break"] = "#B800B3", ["continue"] = "#B800B3", ["do"] = "#B800B3",
	["else"] = "#B800B3", ["elseif"] = "#B800B3", ["end"] = "#B800B3", ["false"] = "#FF0000",
	["for"] = "#B800B3", ["function"] = "#B800B3", ["goto"] = "#B800B3", ["if"] = "#B800B3",
	["in"] = "#B800B3", ["local"] = "#B800B3", ["nil"] = "#B800B3", ["not"] = "#B800B3",
	["or"] = "#B800B3", ["repeat"] = "#B800B3", ["return"] = "#B800B3",
	["then"] = "#B800B3", ["true"] = "#4A00FF", ["until"] = "#B800B3", ["while"] = "#B800B3",

	-- Standard Lua libraries (green)
	["string"] = "#A2FD99", ["math"] = "#A2FD99", ["table"] = "#A2FD99",
	["coroutine"] = "#A2FD99", ["os"] = "#A2FD99", ["io"] = "#A2FD99",
	["debug"] = "#A2FD99", ["utf8"] = "#A2FD99",

	-- Standard Lua functions (blue)
	["assert"] = "#2241FF", ["collectgarbage"] = "#2241FF", ["dofile"] = "#2241FF",
	["error"] = "#2241FF", ["getmetatable"] = "#2241FF", ["ipairs"] = "#2241FF",
	["load"] = "#2241FF", ["loadfile"] = "#2241FF", ["loadstring"] = "#2241FF",
