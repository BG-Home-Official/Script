local T = false
OC.MouseButton1Click:Connect(function()
	T = not T
    Close.Visible = not T
	Clear.Visible = true
    Copy.Visible = true
	Execute.Visible = true
	scrollFrame.Visible = not T
    mainFrame.Draggable = T
    TweenService:Create(mainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Position = UDim2.new(0.52, 0, 0.44, 0)}):Play()
	OC.Text = T and "+" or "-"
    OC.Position = T and UDim2.new(0.9, 0, 0.5, 0) or UDim2.new(0.9, 0, 0, 5)
    OC.AnchorPoint = T and Vector2.new(0.5, 0.5) or Vector2.new(0.5, 0)
    Title.Position = T and UDim2.new(0.5, 0, 0.5, 0) or UDim2.new(0.5, 0, 0, 0)
    Title.AnchorPoint = T and Vector2.new(0.5, 0.5) or Vector2.new(0.5, 0)
