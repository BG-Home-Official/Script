	wait(0.8)
	Frame.Visible = false
	BlackFrame.Visible = false
end)

Confirm.MouseButton1Click:Connect(function()
	local s = Confirm.Size
	if TextLabel.Text == "Do to want to Close?" then
		screenGui:Destroy()
		TweenService:Create(Blur, TweenInfo.new(2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = 0}):Play()
	elseif TextLabel.Text == "Do to want to Copy?" then
		setclipboard(editorBox.Text)
	elseif TextLabel.Text == "Do to want to Execute?" then
		local code = editorBox.Text
		local f, err = loadstring(code)
		if f then
			local ok, runtimeErr = pcall(f)
			if not ok then
				warn("Runtime error:", runtimeErr)
			end
		else
			warn("Syntax error:", err)
		end
	elseif TextLabel.Text == "Do to want to Clear?" then
		editorBox.Text = ""
		highlightLabel.Text = ""
	end
	TweenService:Create(Frame, TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = UDim2.new(0, 0, 0, 0)}):Play()
	TweenService:Create(TextLabel, TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextSize = 0}):Play()
	TweenService:Create(Confirm, TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextSize = 0}):Play()
	TweenService:Create(Decline, TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextSize = 0}):Play()
	TweenService:Create(BlackFrame, TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Transparency = 1}):Play()
	wait(0.8)
	Frame.Visible = false
	BlackFrame.Visible = false
end)

