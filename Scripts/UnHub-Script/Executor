local function escapeRichText(str)
	str = str:gsub("&", "&amp;")
	str = str:gsub("<", "&lt;")
	str = str:gsub(">", "&gt;")
	return str
end

local function highlightSyntax(text)
	text = escapeRichText(text)
	
	text = text:gsub('(".-")', '<font color="#C4FE4A">%1</font>')
	text = text:gsub("('.-')", '<font color="#C4FE4A">%1</font>')
	
	text = text:gsub("(%f[%a_])([%a_][%w_]*)(%f[^%w_])", function(front, word, back)
		local color = keywords[word]
		if color then
			return front .. string.format('<font color="%s">%s</font>', color, word) .. back
		else
			return front .. word .. back
		end
	end)
	
	return text
end

local function updateCanvasSize()
	scrollFrame.CanvasSize = UDim2.new(0, highlightLabel.TextBounds.X + 20, 0, highlightLabel.TextBounds.Y + 20)
end

local function UpdateLineNumbers()
	local lines = string.split(editorBox.Text, "\n")
	local numStr = ""
	for i = 1, #lines do
		numStr = numStr .. i .. "\n"
	end
	LineNumbers.Text = numStr
end

local function updateHighlight()
	wait()
	UpdateLineNumbers()
	editorBox.TextTransparency = 0
	highlightLabel.Text = highlightSyntax(editorBox.Text)
	updateCanvasSize()
end

scrollFrame:GetPropertyChangedSignal("CanvasPosition"):Connect(function()
	local offset = -scrollFrame.CanvasPosition
     updateCanvasSize()
end)

editorBox:GetPropertyChangedSignal("Text"):Connect(updateHighlight)

editorBox.Focused:Connect(function()
	highlightLabel.TextTransparency = 1
     updateCanvasSize()
end)

editorBox.FocusLost:Connect(function()
	highlightLabel.TextTransparency = 0
    updateCanvasSize()
end)

