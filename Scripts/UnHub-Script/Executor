	-- Common string functions (blue)
	["byte"] = "#2241FF", ["char"] = "#2241FF", ["find"] = "#2241FF", ["format"] = "#2241FF",
	["gmatch"] = "#2241FF", ["gsub"] = "#2241FF", ["len"] = "#2241FF", ["lower"] = "#2241FF",
	["match"] = "#2241FF", ["rep"] = "#2241FF", ["reverse"] = "#2241FF",
	["sub"] = "#2241FF", ["upper"] = "#2241FF", ["."] = "#FFFFFF", ["("] = "#FFFFFF", [")"] = "#FFFFFF",

	-- Common math functions (blue)
	["abs"] = "#2241FF", ["acos"] = "#2241FF", ["asin"] = "#2241FF", ["atan"] = "#2241FF",
	["ceil"] = "#2241FF", ["cos"] = "#2241FF", ["deg"] = "#2241FF", ["exp"] = "#2241FF",
	["floor"] = "#2241FF", ["fmod"] = "#2241FF", ["huge"] = "#2241FF", ["log"] = "#2241FF",
	["max"] = "#2241FF", ["min"] = "#2241FF", ["modf"] = "#2241FF", ["pi"] = "#2241FF",
	["rad"] = "#2241FF", ["random"] = "#2241FF", ["sin"] = "#2241FF", ["sqrt"] = "#2241FF",
	["tan"] = "#2241FF",

	-- Roblox globals (green)
	["game"] = "#A2FD99", ["workspace"] = "#A2FD99", ["script"] = "#A2FD99",
	["Enum"] = "#A2FD99", ["Instance"] = "#A2FD99", ["Vector3"] = "#A2FD99",
	["Vector2"] = "#A2FD99", ["UDim"] = "#A2FD99", ["UDim2"] = "#A2FD99",
	["Color3"] = "#A2FD99", ["CFrame"] = "#A2FD99", ["RaycastParams"] = "#A2FD99",
