    if string.find(txt, "http://www.roblox.com") then
        s.SoundId = string.gsub(txt, "http://www.roblox.com/asset/?id=", "rbxassetid://")
    elseif string.find(txt, "rbxassetid://") then
    	s.SoundId = txt
    else
        s.SoundId = "rbxassetid://" ..txt
    end
end})

