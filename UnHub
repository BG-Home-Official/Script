        if player.Name == name then
            if player ~= game.Players.LocalPlayer then
                TargetPlayerAiChat = player
                PlayerShow:Set("Player: " ..player.Name, 0, Color3.fromRGB(255, 255, 255), true)
                if Running then
                    UpdataChat()
                end
                AIChatAll:Set(false)
                break
            else
                Rayfield:Notify({
                    Title = " Warn!",
